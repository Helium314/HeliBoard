                latin:keySpec="&#x0038;" />
            <Key
                latin:keySpec="&#x0039;" />
        </case>
        <default>
            <Key
                latin:keySpec="&#x11ba;"
                latin:keyHintLabel="&#x11c1;"
                latin:additionalMoreKeys="&#x11c1;" />
            <Key
                latin:keySpec="&#x11af;"
                latin:keyHintLabel="&#x11c0;"
                latin:additionalMoreKeys="&#x11c0;" />
            <Key
                latin:keySpec="&#x1167;"
